[library]
version = "@VERSION@"
description = "The library to operate ALSA HwDep character device for Audio and Music unit in IEEE 1394 bus"
authors = "Takashi Sakamoto"
license = "LGPL-2.1-or-later"
website_url = "https://alsa-project.github.io/gobject-introspection-docs/hitaki/"
docs_url = "https://alsa-project.github.io/gobject-introspection-docs/hitaki/"
browse_url = "https://github.com/alsa-project/libhitaki/"
repository_url = "https://github.com/alsa-project/libhitaki.git"
devhelp=true
search_index=true

dependencies = [
  "GLib-2.0",
  "GObject-2.0",
]

related = [
  "Hinawa-3.0",
]

[dependencies."GLib-2.0"]
name = "GLib"
description = "The base utility library"
docs_url = "https://docs.gtk.org/glib/"

[dependencies."GObject-2.0"]
name = "GObject"
description = "The base type system and object class"
docs_url = "https://docs.gtk.org/gobject/"

[related."Hinawa-3.0"]
name = "Hinawa"
description = "The library to operate Linux FireWire character device for IEEE 1394 asynchronous transaction"
docs_url = "https://alsa-project.github.io/gobject-introspection-docs/hinawa"

[source-location]
base_url = "https://github.com/alsa-project/libhitaki/blob/master/"
file_format = "{filename}#L{line}"

[extra]
content_files = [
  "overview.md",
]

content_images = []

urlmap_file = "urlmap.js"
